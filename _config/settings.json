{
  "//": [
    "This is an example Latid config file",
    "tweak it as you want"
  ],
  "site": {
    "title": "Latid Playground",
    "motto": "Examples, snippets, docs. ",
    "url": "https://girobusan.github.io/latid.github.io/index.html"
  },
  "editor" : {
    "content_selector": "#content",
    "default_meta"  :{
      "tags" : ""
    }
  },
  "output": {
    "dir": "docs",
    "time_diff": 0,
    "debug": true,
    "content_selector": "#content",
    "list_max": 10
  },
  "markdown":{
    "fix_links" : true ,
    "force_source" : true
  },
  "tags": {
    "dir": "/tags/"
  },
  "rss": {
    "uri": "/rss.xml",
    "count": 20
  },
  "publish":{
    "command" : "_config/publish.sh",
    "args" : ""
  }
}
